@prefix waterM: <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc:   <http://purl.org/dc/elements/1.1/> .
@base <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl> .

<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl> rdf:type owl:Ontology;
    rdfs:label "WaterM Ontology"@en;
  dc:title "WaterM Ontology"@en;
    dc:created "2018-01-10"^^xsd:date;
    dc:creator "François Calvier", "Kamal Singh", "Abderrahmen kammoun", "Amro Najjar";
    dc:description "The WaterM ontology aims to model water quality monitoring and management in water network"@en;
    owl:versionInfo "1.1" ;
  dc:rights "Copyright © 2018 The Water-M project".
  
#################################################################
#    Annotation properties
#################################################################

###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Description
:Description rdf:type owl:AnnotationProperty ;
                                 rdfs:subPropertyOf rdfs:comment .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Purpose
:Purpose rdf:type owl:AnnotationProperty ;
                             rdfs:subPropertyOf rdfs:comment .


#################################################################
#    Object Properties
#################################################################

###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/canAccess
:canAccess rdf:type owl:ObjectProperty ;
                               rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Staff> ;
                               rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/compose
:compose rdf:type owl:ObjectProperty ;
                             owl:inverseOf :isComposedOf ;
                             rdf:type owl:AsymmetricProperty ,
                                      owl:TransitiveProperty ,
                                      owl:IrreflexiveProperty ;
                             rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device> ;
                             rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/concern
:concern rdf:type owl:ObjectProperty ;
                             rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident> ;
                             rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/energySupply
:energySupply rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf :supply ;
                                  owl:inverseOf :isEnergySuppliedBy ;
                                  rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Generator> ;
                                  rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/ElectricDevice> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/hasOrigin
:hasOrigin rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf owl:topObjectProperty ;
                               rdf:type owl:IrreflexiveProperty ;
                               rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Measurement> ;
                               rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Sensor> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/hasTarget
:hasTarget rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf owl:topObjectProperty ;
                               rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Measurement> ;
                               rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/DataCenter> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/hasUnit
:hasUnit rdf:type owl:ObjectProperty ;
                             rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Measurement> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/induce
:induce rdf:type owl:ObjectProperty ;
                            owl:inverseOf :solve ;
                            rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident> ;
                            rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Intervention> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/isComposedOf
:isComposedOf rdf:type owl:ObjectProperty ,
                                           owl:AsymmetricProperty ,
                                           owl:TransitiveProperty ,
                                           owl:IrreflexiveProperty .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/isEnergySuppliedBy
:isEnergySuppliedBy rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf :isSupliedBy ;
                                        rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Generator> ;
                                        rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Generator> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/isRealizedBy
:isRealizedBy rdf:type owl:ObjectProperty ;
                                  owl:inverseOf :realize ;
                                  rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Intervention> ;
                                  rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Staff> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/isReportedBy
:isReportedBy rdf:type owl:ObjectProperty ;
                                  owl:inverseOf :report ;
                                  rdf:type owl:FunctionalProperty ;
                                  rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/isResponsibleFor
:isResponsibleFor rdf:type owl:ObjectProperty ;
                                      rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Manager> ;
                                      rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/isSupliedBy
:isSupliedBy rdf:type owl:ObjectProperty ;
                                 owl:inverseOf :supply .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/isWaterSuppliedBy
:isWaterSuppliedBy rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf :isSupliedBy ;
                                       owl:inverseOf :waterSupply ;
                                       rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/WaterDevice> ;
                                       rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/WaterDevice> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/realize
:realize rdf:type owl:ObjectProperty ;
                             rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Staff> ;
                             rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Intervention> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/report
:report rdf:type owl:ObjectProperty ,
                                     owl:InverseFunctionalProperty ,
                                     owl:IrreflexiveProperty ;
                            rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/respondTo
:respondTo rdf:type owl:ObjectProperty ;
                               rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Intervention> ;
                               rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/solve
:solve rdf:type owl:ObjectProperty ;
                           rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Intervention> ;
                           rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/supply
:supply rdf:type owl:ObjectProperty ,
                                     owl:IrreflexiveProperty .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/waterSupply
:waterSupply rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf :supply ;
                                 rdf:type owl:TransitiveProperty ,
                                          owl:IrreflexiveProperty ;
                                 rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/WaterDevice> ;
                                 rdfs:range <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/WaterDevice> .


#################################################################
#    Data properties
#################################################################

###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/InstallationDate
:InstallationDate rdf:type owl:DatatypeProperty ;
                                      rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device> ;
                                      rdfs:range xsd:dateTime .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/altitude
:altitude rdf:type owl:DatatypeProperty ;
                              rdfs:subPropertyOf :geographicCoordinates .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/consumption
:consumption rdf:type owl:DatatypeProperty ;
                                 rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device> ;
                                 rdfs:range xsd:float .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/diameter
:diameter rdf:type owl:DatatypeProperty ;
                              rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/WaterDevice> ;
                              rdfs:range xsd:float .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/electricConsumption
:electricConsumption rdf:type owl:DatatypeProperty ;
                                         rdfs:subPropertyOf :consumption ;
                                         rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/ElectricDevice> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/fuelConsumption
:fuelConsumption rdf:type owl:DatatypeProperty ;
                                     rdfs:subPropertyOf :consumption ;
                                     rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Generator> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/geographicCoordinates
:geographicCoordinates rdf:type owl:DatatypeProperty ;
                                           rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/hasDate
:hasDate rdf:type owl:DatatypeProperty ;
                             rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Measurement> ;
                             rdfs:range xsd:dateTimeStamp .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/hasMaxValue
:hasMaxValue rdf:type owl:DatatypeProperty ;
                                 rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Sensor> ;
                                 rdfs:range owl:real .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/hasMinValue
:hasMinValue rdf:type owl:DatatypeProperty ;
                                 rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Sensor> ;
                                 rdfs:range owl:real .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/hasValue
:hasValue rdf:type owl:DatatypeProperty ;
                              rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Measurement> ;
                              rdfs:range xsd:float .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/lattitude
:lattitude rdf:type owl:DatatypeProperty ;
                               rdfs:subPropertyOf :geographicCoordinates .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/longitude
:longitude rdf:type owl:DatatypeProperty ;
                               rdfs:subPropertyOf :geographicCoordinates .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/rotationSpeed
:rotationSpeed rdf:type owl:DatatypeProperty ;
                                   rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Fan> ,
                                               <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Pump> ;
                                   rdfs:range xsd:float .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/volume
:volume rdf:type owl:DatatypeProperty ;
                            rdfs:domain <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Tank> .


#################################################################
#    Classes
#################################################################

###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Accelerator
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Accelerator> rdf:type owl:Class ;
                                                                           rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/PumpingGroup> ;
                                                                           :Description "This is a subclass of the PumpingGroup class, it models an accelerator used to increase the pressure in the network."^^xsd:string ;
                                                                           :Purpose "Sometimes while the flow rate is enough, the pressure might be too weak to ensure a comfortable distribution for users. For this reason, a pump (or multiple pumps) are positioned to pump water from upstream to downstream."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/AirQualitySensor
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/AirQualitySensor> rdf:type owl:Class ;
                                                                                rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/AirSensor> .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/AirSensor
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/AirSensor> rdf:type owl:Class ;
                                                                         rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/EnvironmentSensor> ;
                                                                         :Description "This class is subclass of the environment sensor, it senses air quality in a given facility or location."^^xsd:string ;
                                                                         :Purpose "The quality of the air in a reservoir or a facility can be an important indicator of the water quality."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Alert
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Alert> rdf:type owl:Class ;
                                                                     rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident> ;
                                                                     :Description "This class is a subclass of the class Incident. It is a generic class to represent an alert."^^xsd:string ;
                                                                     :Purpose "An alert is reported by one of the staff members or by a data-analysis process."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Basin
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Basin> rdf:type owl:Class ;
                                                                     rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Reservoir> ;
                                                                     :Description "This is a subclass of the Reservoir class. It models a Basin." ;
                                                                     :Purpose "a reservoir that exploit the topography to create the pressure" .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Bladder
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Bladder> rdf:type owl:Class ;
                                                                       rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/WaterDevice> ,
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty :isComposedOf ;
                                                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                         owl:onClass <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Manometer>
                                                                                       ] ;
                                                                       :Description "This is a subclass of the water device, it represents a bladder."^^xsd:string ;
                                                                       :Purpose "A bladder is a flexible water pouch associated with a pump"^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Booster
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Booster> rdf:type owl:Class ;
                                                                       rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/PumpingGroup> ,
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty :isComposedOf ;
                                                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                         owl:onClass <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Bladder>
                                                                                       ] ;
                                                                       :Description "This is a subclass of the PumpingGroup class, it model a booster."^^xsd:string ;
                                                                       :Purpose "When a booster is used, the users are provided directly by the pumps taking the water reserve and inflating a sealed bladder ensuring a minimum buffer stock. When the pressure within the bladder drops below a set value the pumps are again put in operation."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Claim
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Claim> rdf:type owl:Class ;
                                                                     rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident> ,
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty :isReportedBy ;
                                                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                       owl:onClass <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Client>
                                                                                     ] ;
                                                                     :Description "This is a subclass of the Incident Class. it is a generic class for claims raised either by the water utility or the users."^^xsd:string ;
                                                                     :Purpose "Allowing to model claims."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Client
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Client> rdf:type owl:Class ;
                                                                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty :report ;
                                                                                        owl:someValuesFrom <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident>
                                                                                      ] ;
                                                                      :Description "This is a general client of the water distribution company."^^xsd:string ;
                                                                      :Purpose "This class allows clients to have iDs,  visualize their consumption, their accounts, etc."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Collectivity
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Collectivity> rdf:type owl:Class ;
                                                                            rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Client> ;
                                                                            :Description "Collectivity, a subtype of client. Typically, it remains the organising entity of the service even though it chooses to delegate the water-distribution service to a private company."^^xsd:string ;
                                                                            :Purpose "Represents the collectivity, allows it to ensure the quality of the water. It receives information from national, provincial bodies or users, analyze this information and may report them to the water agency, the state or to the european level. The collectivity may also raise alerts."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/ConsumableRenewal
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/ConsumableRenewal> rdf:type owl:Class ;
                                                                                 rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Intervention> ;
                                                                                 :Description "Some consumable in the network may be used up or may reach their expiry date, in this case a renewal intervention  should be conducted."^^xsd:string ,
                                                                                                                  "This class is a subclass of the intervention class, it models, an intervention to renew  a consumable."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Controler
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Controler> rdf:type owl:Class ;
                                                                         rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/WaterDevice> ,
                                                                                         [ rdf:type owl:Restriction ;
                                                                                           owl:onProperty :isComposedOf ;
                                                                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                           owl:onClass <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Flowmeter>
                                                                                         ] ,
                                                                                         [ rdf:type owl:Restriction ;
                                                                                           owl:onProperty :isComposedOf ;
                                                                                           owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                                           owl:onClass <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Manometer>
                                                                                         ] ;
                                                                         :Description "This is a generic class representing a controller. This place could be a sub-class of the class actuator of the SSN ontology. The difference between this class and the class actuator can be seen as follows: while Actuator is qualified as an object that changes the status of the system, controller is capable of changing the system status depending on measurements it has already done on the system ."^^xsd:string ;
                                                                         :Purpose "It is used to control some parameters to ensure their value falls into the acceptable range.  Note that this class involves a full control loop. it can have one or multiple sensors and one or multiple actuators working on the system."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/DataCenter
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/DataCenter> rdf:type owl:Class ;
                                                                          rdfs:subClassOf <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/ElectricDevice> ,
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty :report ;
                                                                                            owl:someValuesFrom <https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Incident>
                                                                                          ] ;
                                                                          :Description "This class is a usb class of electric device, it models a data center or servers used to stock and process data."^^xsd:string ;
                                                                          :Purpose "A data center is used to stock and process data collected by the sensors."^^xsd:string .


###  https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device
<https://github.com/WaterMOntology/WaterMOntology/blob/master/WaterM.ttl/Device> rdf:type owl:Class ;
